if (pembayaran >= totalBiaya) {
             double kembalian = pembayaran - totalBiaya;
             System.out.println("Pembayaran diterima. Kembalian: Rp" + kembalian);
             System.out.println("Terima kasih telah melakukan booking!");
         } else {
